{
  "name": "guardianconnector-explorer",
  "version": "3.0.0",
  "type": "module",
  "description": "A Nuxt app to explore GuardianConnector data in different views",
  "scripts": {
    "build": "nuxt build",
    "dev": "nuxt dev",
    "format": "prettier \"**/*.{js,ts,jsx,tsx,vue}\" --write",
    "generate": "nuxt generate",
    "link-module": "pnpm link ../gc-shared-components",
    "lint:prettier": "prettier ./ --check --ignore-path .prettierignore",
    "lint:eslint": "eslint . --cache",
    "lint": "pnpm run lint:prettier && pnpm run lint:eslint",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "test": "vitest run",
    "prepare": "husky"
  },
  "dependencies": {
    "@turf/turf": "^7.2.0",
    "@vojtechlanka/vue-tags-input": "^3.1.1",
    "chart.js": "^4.4.7",
    "gc-shared-resources": "latest",
    "lodash": "^4.17.21",
    "mapbox-gl": "^3.9.4",
    "mapbox-gl-ruler-control": "^0.0.2",
    "nuxt": "^3.15.4",
    "nuxt-auth-utils": "^0.3.6",
    "pg": "^8.13.1",
    "sqlite3": "^5.1.7",
    "tokml": "^0.4.0",
    "vue": "latest",
    "vue-3-slider-component": "^1.0.1",
    "vue-chartjs": "^5.3.2",
    "vue-datepicker-next": "^1.0.3",
    "vue-router": "latest",
    "vue3-select-component": "^0.9.0"
  },
  "lint-staged": {
    "./**/*.{js,jsx,ts,tsx,json,css,vue}": [
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@eslint/js": "^9.19.0",
    "@nuxt/test-utils": "^3.15.4",
    "@nuxtjs/i18n": "^8.5.5",
    "@types/mapbox-gl": "^3.4.1",
    "@types/node": "^22.13.1",
    "@types/pg": "^8.11.11",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.19.0",
    "eslint-config-flat-gitignore": "^2.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "globals": "^15.14.0",
    "husky": "^9.1.7",
    "jsdom": "^26.0.0",
    "lint-staged": "^15.4.3",
    "nuxt-windicss": "^3.0.1",
    "prettier": "^3.4.2",
    "typescript-eslint": "^8.23.0",
    "vitest": "^3.0.5"
  },
  "engines": {
    "node": "20.15.0"
  },
  "author": "Conservation Metrics",
  "license": "MIT"
}
